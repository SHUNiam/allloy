{
  "version": 2,
  "regions": ["hnd1"],
  "builds": [
    { "src": "app.js","use": "@vercel/node" },
    { "src": "public/assets/*.js","use": "@vercel/node" }
    { "src": "*.html","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/public/index.html" },
    { "src": "/.*","dest": "/app.js" }
  ]
}
