{
  "version": 2,
  "regions": ["iad1"],
  "builds": [
    { "src": "server.js","use": "@vercel/node" },
    { "src": "public/assets/*.js","use": "@vercel/node" },
    { "src": "*.html","use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/","dest": "/public/index.html" },
    { "src": "/.*","dest": "/public/assets/main.js" }
  ]
}
